{procedure TForm2.Edit1ChangeTracking(Sender: TObject);

var
thetext:String;
begin

 thetext := Edit1.Text;
 Edit1.OnChangeTracking := nil;
 Edit1.Text := '';
 Edit1.Text := AnsiUpperCase(thetext);

 Edit1.OnChangeTracking := Edit1ChangeTracking;
 Edit1.GoToTextEnd;
end;

