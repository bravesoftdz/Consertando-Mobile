            {begin
                If Label3.Text = '' Then
                  begin
                    Label4.Visible := True;
                    Label4.Text := 'Informe o nome corretamente e com portugues correto' + #13 + '(Verifique os espaços).';
                  End;
            end;}
            Except
                begin
                Label3.Visible := False;
                Label4.Visible := true;
