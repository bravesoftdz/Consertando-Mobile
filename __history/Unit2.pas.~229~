                    Label3.Text := '1- Troque a memória(EEPROM)';
                End;
              begin
                If (Edit1.Text = 'LIGA SOZINHO') Or (Edit1.Text = 'LIGA SOZINHA') Or (Edit1.Text = 'FICA LIGANDO SOZINHO') Or (Edit1.Text = 'FICA LIGANDO SOZINHA') Or (Edit1.Text = 'ESTÁ LIGANDO SOZINHA') Or (Edit1.Text = 'ESTA LIGANDO SOZINHA') Or (Edit1.Text = 'ESTÁ LIGANDO SOZINHO') Or (Edit1.Text = 'ESTA LIGANDO SOZINHA') Or (Edit1.Text = 'TV LIGA SOZINHA') Or (Edit1.Text = 'LIGA') Or (Edit1.Text = 'LIGA DO NADA') Or (Edit1.Text = 'TV LIGA DO NADA') Then

                    Label3.Text := '1- Troque a memória(EEPROM)';
                End;
              begin
